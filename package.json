{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "logger": "pnpm -F @repo/logger",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "catalog:",
    "turbo": "^2.7.2",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22"
  }
}
